ds-monitor/src/App.js",["59"],[],"import React, {useState, useEffect} from 'react'\nimport './App.css';\nimport RDSMonitor from './components/RDSMonitor'\nimport axios from 'axios'\nimport {BrowserRouter as Router, Route, Link, Routes, useNavigate} from 'react-router-dom'\n\n\nfunction App() {\n  const base_url = 'http://awsrdsmaintenancedashboards.mmm.com/api'\n  const [accounts, getAccounts] = useState([])\n  useEffect(()=>{\n    axios.get(base_url+\"/list-accounts\").then(monitor=>{\n        getAccounts(monitor.data)\n      }).catch(error => {\n          console.log(error)\n      })\n  }, [])\n  return (\n    <div className=\"App\">\n          <Router>\n            <header>\n              <ul>\n                {accounts.map(account => (<li key={account}><Link to={'account/' + account}>{account}</Link></li>))}\n              </ul>\n            </header>\n            <Routes>\n              <Route path=\"account/:account_name\" element={<RDSMonitor base_url={base_url} />} />\n            </Routes>\n          </Router>\n    </div>\n  );\n}\n\nexport default App;\n","/home/sachin/rds_monitor/react-app/rds-monitor/src/components/RDSMonitor.js",["60","61"],[],"/home/sachin/rds_monitor/react-app/rds-monitor/src/components/Metrix.js",["62","63"],[],"import React from \"react\";\nimport { Bar } from \"react-chartjs-2\";\nimport { Chart, registerables } from 'chart.js';\nChart.register(...registerables);\n\nfunction Metrix({metrix_data, account_name}){\n    return(\n        <div>\n            {metrix_data.map(matrix =>(<div key={matrix.request_id} style={{ maxWidth: \"80%\", \"textAlign\": \"center\", margin: \"0 auto\" }}>\n                <div style={{float: \"left\"}}><a href={\"#top\"}>Go to Top^</a></div>\n                <h3><label id={matrix[\"instance_identifier\"]}> Account: {matrix[\"instance_identifier\"]}</label></h3>\n                <Bar\n                    data={{\n                        // Name of the variables on x-axies for each bar\n                        labels: matrix[\"MetricDataResults\"][0][\"Timestamps\"].map(formatDate),\n                        datasets: [\n                        {\n                            // Label for bars\n                            label: \"FreeStorageSpace(in GB)/datetime\",\n                            // Data or value of your each variable\n                            data: matrix[\"MetricDataResults\"][0][\"Values\"].map(data=>bytesToSize(parseInt(data))),\n                            borderWidth: 0.1,\n                            borderColor: [\"#f55d5d\"],\n                            backgroundColor: [\"#f55d5d\"],\n                        },\n                        ],\n                    }}\n                    // Height of graph\n                    height={80}\n                />\n            \n            </div>))}\n        </div>\n    )\n}\nfunction bytesToSize(bytes) {\n    var i = Math.floor(Math.log(bytes) / Math.log(1024)),\n    sizes = ['B', 'KB', 'MB', 'GB', 'TB', 'PB', 'EB', 'ZB', 'YB'];\n    return ((bytes / Math.pow(1000, 2)).toFixed(2) * 1)/1000;\n}\n\nfunction formatDate(date_obj) {\n    return date_obj.replace('+00:00', '')\n}\nexport default Metrix",{"ruleId":"64","severity":1,"message":"65","line":5,"column":55,"nodeType":"66","messageId":"67","endLine":5,"endColumn":66},{"ruleId":"64","severity":1,"message":"68","line":36,"column":9,"nodeType":"66","messageId":"67","endLine":36,"endColumn":10},{"ruleId":"64","severity":1,"message":"69","line":37,"column":5,"nodeType":"66","messageId":"67","endLine":37,"endColumn":10},{"ruleId":"64","severity":1,"message":"65","line":5,"column":55,"nodeType":"66","messageId":"67","endLine":5,"endColumn":66},{"ruleId":"70","severity":1,"message":"71","line":20,"column":8,"nodeType":"72","endLine":20,"endColumn":22,"suggestions":"73"},{"ruleId":"70","severity":1,"message":"71","line":27,"column":8,"nodeType":"72","endLine":27,"endColumn":22,"suggestions":"74"},{"ruleId":"64","severity":1,"message":"68","line":37,"column